class inputbuffer_t

private function SplitInput(string& command, vector<string>& args) : void

	string word;
	vector<string> temp_args;
	
	for int i = 1, i <= buffer.length(), i++
		if(buffer[i] != ' ')
			word += buffer[i]
		else if(s[i] == ' ' )
			args.pushback(word)
			word.clear()
		end if
	end for
	
	command = args.at(0);
	args.erase(args.begin())
	
